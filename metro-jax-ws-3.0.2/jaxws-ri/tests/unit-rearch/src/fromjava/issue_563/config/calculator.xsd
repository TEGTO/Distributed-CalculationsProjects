<xs:schema elementFormDefault="qualified" attributeFormDefault="unqualified" targetNamespace="urn:calculator" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:calculator="urn:calculator">
  <xs:element name="Query">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Arg0" type="xs:integer"/>
        <xs:element name="Arg1" type="xs:integer"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Result">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Sum" type="xs:integer"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
