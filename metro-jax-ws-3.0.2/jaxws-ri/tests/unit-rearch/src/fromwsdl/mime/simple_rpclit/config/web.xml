<web-app>
    <display-name>fromwsdl.mime.simple_rpclit</display-name>
    <description>fromwsdl.mime_simple_rpclit</description>
  
    <listener>
        <listener-class>com.sun.xml.ws.transport.http.servlet.WSServletContextListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>mime_simple_rpclit</servlet-name>
        <display-name>mime_simple_rpclit</display-name>
