<endpoints
    xmlns="http://java.sun.com/xml/ns/jax-ws/ri/runtime"
    version="2.0">

    <endpoint
        name="simple1"
        interface="fromwsdl.multiservice.server.PingPort1"
        implementation="fromwsdl.multiservice.server.PingImpl1"
        wsdl="WEB-INF/wsdl/PingService.wsdl"
        service="{http://xmlsoap.org/Ping}PingService1"
        port="{http://xmlsoap.org/Ping}Ping1"
        url-pattern="/simple1" />


   <endpoint
        name="simple"
        interface="fromwsdl.multiservice.server.PingPort"
        implementation="fromwsdl.multiservice.server.PingImpl"
        wsdl="WEB-INF/wsdl/PingService.wsdl"
        service="{http://xmlsoap.org/Ping}PingService"
        port="{http://xmlsoap.org/Ping}Ping"
        url-pattern="/simple" />

</endpoints>


